{{x-tree-node
  model=model
  select=select
  hover=hover
  hoverOut=hoverOut
  checkable=checkable
  chosenId=chosenId}}

{{#if model.isExpanded}}
  {{x-tree-branch
    model=model.children
    select=select
    hover=hover
    hoverOut=hoverOut
    checkable=checkable
    chosenId=chosenId}}
{{/if}}
